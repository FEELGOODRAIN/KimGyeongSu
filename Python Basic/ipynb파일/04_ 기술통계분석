icks.\n","      quickchartButtonEl.classList.add('colab-df-spinner');\n","      try {\n","        const charts = await google.colab.kernel.invokeFunction(\n","            'suggestCharts', [key], {});\n","      } catch (error) {\n","        console.error('Error during call to suggestCharts:', error);\n","      }\n","      quickchartButtonEl.classList.remove('colab-df-spinner');\n","      quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","    }\n","    (() => {\n","      let quickchartButtonEl =\n","        document.querySelector('#df-73bbd6f2-ebbb-47cd-b830-d35ecfda8893 button');\n","      quickchartButtonEl.style.display =\n","        google.colab.kernel.accessAllowed ? 'block' : 'none';\n","    })();\n","  </script>\n","</div>\n","    </div>\n","  </div>\n"]},"metadata":{},"execution_count":23}]},{"cell_type":"code","source":["\n","df.loc[df['species']=='satosa' , 'species'] = 'setosa'\n","df.loc[df['species']==1 , 'species'] = 'versicolor'\n","df.loc[df['species']==2 , 'species'] = 'virginica'\n","\n","\n"],"metadata":{"id":"8RlR77ztcl7S","executionInfo":{"status":"ok","timestamp":1696554040210,"user_tz":-540,"elapsed":315,"user":{"displayName":"김경수","userId":"10907451516248175311"}}},"execution_count":37,"outputs":[]},{"cell_type":"code","source":["df['species'].astype"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"FAloy7GKcoIZ","executionInfo":{"status":"ok","timestamp":1696554062075,"user_tz":-540,"elapsed":5,"user":{"displayName":"김경수","userId":"10907451516248175311"}},"outputId":"13cb40b6-acc9-409d-a9d2-9a86493dc100"},"execution_count":39,"outputs":[{"output_type":"execute_result","data":{"text/plain":["dtype('O')"]},"metadata":{},"execution_count":39}]},{"cell_type":"code","source":["#모든 값이 평균 0 분산 1인 정규분포에 가깝게\n","scaler.fit(df.loc[:,'sepal length (cm)':'petal width (cm)'])\n","iris_sc = scaler.transform(df.loc[:,'sepal length (cm)':'petal width (cm)'])\n","iris_sc = pd.DataFrame(data = iris_sc , columns = iris.feature